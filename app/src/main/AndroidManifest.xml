<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quizapp_new">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizApp_new">
        <activity
            android:name="com.example.quizapp_new.Controller.activities.TeachersQuestionsActivity"
            android:exported="true" />
        <activity
            android:name="com.example.quizapp_new.Controller.activities.Test"
            android:exported="true" />
        <activity
            android:name="com.example.quizapp_new.Controller.activities.StudentHomeActivity"
            android:exported="true"
            android:launchMode="singleTop" />
        <activity
            android:name="com.example.quizapp_new.Controller.activities.LandingPage"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.quizapp_new.Controller.activities.ResultActivity"
            android:exported="true"
            android:noHistory="true" />
        <activity
            android:name="com.example.quizapp_new.Controller.activities.StartQuizActivity"
            android:exported="false"
            android:noHistory="true" />
        <activity
            android:name="com.example.quizapp_new.Controller.activities.MainActivity"
            android:exported="false"
            android:label="@string/title_activity_home" />
        <activity
            android:name=".Controller.activities.TeachersHomeActivity"
            android:exported="true"
            android:theme="@style/Theme.QuizApp_new.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>